%body
  / -header start
  #slide-nav.navbar.navbar-inverse.navbar-fixed-top.navigation_bar{role: "navigation"}
    .container-fluid
      .navbar-header
        %button.navbar-toggle{"data-target" => "#b-menu-1", "data-toggle" => "collapse", type: "button"}
          %span.sr-only Toggle navigation
          %span.icon-bar
          %span.icon-bar
          %span.icon-bar
        %a.navbar-brand{href: "#"}
          = image_tag("logo.png")
      #b-menu-1.collapse.navbar-collapse
        %ul.nav.navbar-nav.navbar-right
          %li
            %a.navigation_bar_a{href: "#"} HELP
          %li.dropdown.active
            %a.dropdown-toggle.navigation_bar_a{"data-toggle" => "dropdown", href: "#"}
              %span.user_icon
              John Doe
              %b.caret.dropdown_arrow
            %ul.dropdown-menu.dropdown_box
              .dropdown_box_open_arrow
              %li
                %a.dropdown_a{href: "#"}
                  %span.dropdown_a_icon y
                  %span.dropdown_a_text
                    Preferences
                    %br>/
                    Adjust the app to suit your need
              %li
                %a.dropdown_a{href: "#"}
                  %span.dropdown_a_icon A
                  %span.dropdown_a_text
                    Account Settings
                    = succeed "Make" do
                      %br/
              %li
                %a.dropdown_a{href: "#"}
                  %span.dropdown_a_icon :
                  %span.dropdown_a_text
                    Billing
                    %br>/
                    Manage credit cards
              %li
                %a.dropdown_a{href: "#"}
                  %span.dropdown_a_icon Q
                  %span.dropdown_a_text Logout
  / -header end
  .clearfix
  / -Middle container Start
  .container-fluid.middle_container
    .row
      / -left sidebar start
      .left_sidebar
        %nav.navbar
          .navbar-header
            %button.navbar-toggle.navigation_toggle{"data-target" => "#b-menu-2", "data-toggle" => "collapse", type: "button"}
              %span.sr-only Toggle navigation
              %span.icon-bar
              %span.icon-bar
              %span.icon-bar
          #b-menu-2.collapse.navbar-collapse.navigation-collapse
            %ul.nav.navbar-nav
              %li
                %a{href: "#"}
                  %i.result
                  %span Result
              %li
                %a{href: "#"}
                  %i.customer
                  %span customer
              %li
                %a{href: "#"}
                  %i.review
                  %span Reviews
              %li
                %a{href: "#"}
                  %i.campaigns
                  %span Campaigns
              %li
                %a{href: "#"}
                  %i.appointments
                  %span Appointments
              %li
                %a{href: "#"}
                  %i.settings
                  %span Settings
      / -left sidebar end
      / -right section start
      .right_section.row
        .row.page_heading
          %ol.breadcrumb
            %li
              %a{href: "#"} Acme Enterprise
            %li
              %a{href: "#"} Contacts
            %li.active Contact Detail
        / -contact detail page start
        .row.detail
          .col-md-5.col-xs-12.col-sm-6.contact_detail
