              .col-md-12.col-xs-12.col-sm-12
                %h5 CONTACT DETAILS
            .row
              .col-md-6.col-xs-6.col-sm-6
                %h6 First Name
                %h5 John Michael
              .col-md-6.col-xs-6.col-sm-6
                %h6 Last Name
                %h5 Vincent Doe
            .row
              .col-md-6.col-xs-12.col-sm-12
                %h6 Email
                %h5 john.vincent.doe@me.com
              .col-md-6.col-xs-6.col-sm-6
            .row
              .col-md-6.col-xs-6.col-sm-6
                %h6 Phone Number
                %h5 (888) 555-6677-8899
              .col-md-6.col-xs-6.col-sm-6
                %h6 Mobile Phone Number
                %h5 999) 222-3344-5566
            .row
              .col-md-6.col-xs-6.col-sm-6
                %h6 Address 1
                %h5 333 Battery Street
              .col-md-6.col-xs-6.col-sm-6
                %h6 Address 2
                %h5 333 Battery Street
            .row
              .col-md-6.col-xs-6.col-sm-6
                %h6 City
                %h5 San Francisco
              .col-md-6.col-xs-6.col-sm-6
                %h6 States
                %h5 California
            .row
              .col-md-6.col-xs-6.col-sm-6
                %h6 Zip
                %h5 94111
              .col-md-6.col-xs-6.col-sm-6
                %h6 Country
                %h5 United States
            .row
              .col-md-12.col-xs-12.col-sm-12
                %h6 Communication Preferences
            .row
              .col-md-12.col-xs-12.col-sm-12.communication_preferences
                %ul
                  %li
                    %span.email_icon
                    Email
                    %span.glyphicon.glyphicon-ok.ok_icon
                  %li
                    %span.sms_icon
                    SMS
                    %button.close.sms_close_icon{"aria-hidden" => "true", type: "button"} Ã—
                  %li
                    %span.direct_mail_icon
                    Direct Mail
                    %span.glyphicon.glyphicon-ok.ok_icon
            %a.btn.btn-success.update_btn{href: "#", onclick: "document.getElementById('update_contact_underlay').style.display='block'; document.getElementById('update_contact_lightbox').style.display='block';"}
              %b UPDATE CONTACT
          / - UPDATE CONTACT lightbox content start
          #update_contact_underlay{onclick: "document.getElementById('update_contact_underlay').style.display='none'; document.getElementById('update_contact_lightbox').style.display='none';"}
          #update_contact_lightbox
            %a.close_btn{href: "#", onclick: "document.getElementById('update_contact_underlay').style.display='none'; document.getElementById('update_contact_lightbox').style.display='none';"}
            .row.update_contact_top_heading
              %h5 UPDATE CONTACT
            .row.main_row
              .col-md-3.col-sm-3.col-xs-6.text-right First Name
              .col-md-3.col-sm-3.col-xs-5
                %input.col-md-12{type: "text", value: "John Michael"}/
              .col-md-3.col-sm-3.col-xs-6.text-right Last Name
              .col-md-3.col-sm-3.col-xs-5
                %input.col-md-12{type: "text", value: "Vincent Doe"}/
            .row.main_row
              .col-md-5.col-sm-5.col-xs-4.text-right
                %span CONTACT
              .col-md-7.col-sm-7.col-xs-8.text-right
                %hr/
            .row.main_row
              .col-md-3.col-sm-3.col-xs-6.text-right Email
              .col-md-8.col-sm-8.col-xs-5
                %input.col-md-12{type: "text", value: "john.vincent.doe@me.com"}/
              .col-md-1.col-sm-1.col-xs-12
            .row.main_row
              .col-md-3.col-sm-3.col-xs-6.text-right Phone Number
              .col-md-3.col-sm-3.col-xs-5
                %input.col-md-10{type: "text", value: "(510) 230-0588"}/
              .col-md-3.col-sm-3.col-xs-6.text-right Mobile Phone Number
              .col-md-3.col-sm-3.col-xs-5
                %input.col-md-10{type: "text", value: "(510) 230-0588"}/
            .row.main_row
              .col-md-5.col-sm-5.col-xs-4.text-right
                %span ADDRESS
              .col-md-7.col-sm-7.col-xs-8.text-right
                %hr/
            .row.main_row
              .col-md-3.col-sm-3.col-xs-6.text-right Line1
              .col-md-4.col-sm-4.col-xs-5
                %input.col-md-12{type: "text", value: "2020 Milvia St. #400"}/
              .col-md-1.col-sm-1.col-xs-6.text-right Line2
              .col-md-4.col-sm-4.col-xs-5
                %input.col-md-12{type: "text", value: "S"}/
            .row.main_row
              .col-md-3.col-sm-3.col-xs-6.text-right City
              .col-md-4.col-sm-4.col-xs-5
                %input.col-md-12{type: "text", value: "Berkeley"}/
              .col-md-1.col-sm-1.col-xs-6.text-right State
              .col-md-4.col-sm-4.col-xs-5
                %select
                  %option California
                  %option India
                  %option Bharat
            .row.main_row
              .col-md-3.col-sm-3.col-xs-6.text-right Zip
              .col-md-4.col-sm-4.col-xs-5
                %input.col-md-8{type: "text", value: "94704"}/
              .col-md-1.col-sm-1.col-xs-6.text-right Country
              .col-md-4.col-sm-4.col-xs-5
                %select
                  %option United States
                  %option India
                  %option Bharat
            .row.main_row
              .col-md-3.col-sm-3.col-xs-6.text-right
              .col-md-3.col-sm-4.col-xs-6
                %button.btn.btn-success.col-md-12
